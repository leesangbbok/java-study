package ex7;

class SwitchClass{
	public String getDdi(int i){
		
		String ddi = "";
		
		switch(i % 12) {
			case 0 : ddi = "¿ø¼þÀÌ";
			break;
			case 1 : ddi = "´ß";
			break;
			case 2 : ddi = "°³";
			break;
			case 3 : ddi = "µÅÁö";
			break;
			case 4 : ddi = "Áã";
			break;
			case 5 : ddi = "¼Ò";
			break;
			case 6 : ddi = "È£¶ûÀÌ";
			break;
			case 7 : ddi = "Åä³¢";
			break;
			case 8 : ddi = "¿ë";
			break;
			case 9 : ddi = "¹ì";
			break;
			case 10 : ddi = "¸»";
			break;
			case 11 : ddi = "¾ç";
			break;
		}
		return ddi+ "¶ì";
	}
}

public class SwitchTest {
	
	public static void main(String[] args) {
		SwitchClass sc = new SwitchClass();
		
		String ddiStr = sc.getDdi(1950);
		
		System.out.println(ddiStr);
		
	}

}
