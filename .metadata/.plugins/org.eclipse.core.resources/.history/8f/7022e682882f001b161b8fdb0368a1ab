package ex9;

import java.util.Scanner;

public class RepeatTest {

	public static void main(String[] args) {
			
			for (int i =1; i <= 10 ; i++) {
				System.out.print(i + ". IU");
				System.out.print("\n");
			}
			
			//무한 반복
			for (;;) {
				System.out.println("IU");
			}
			
			int coin;
			Scanner sc = new Scanner(System.in);
			
	}

}
