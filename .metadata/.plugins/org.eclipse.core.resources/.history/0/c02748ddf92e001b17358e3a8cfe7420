package ex7;

public class SwitchTest2 {
	
	//점수 90 ~100 : A
	//점수 80 ~99 :   B
	//점수 70 ~ 89:   C
	//점수 60 ~ 79 :  D
	//점수 0 ~ 59 :    F
	
	
	public static void main(String[] args) {
		 int score = 60;
		 char grade = ' ';
		
//		 if(score >= 90) {
