package ex9;

class Star{
	//star1 함수
	public void star1() {
		System.out.println("* * * * *");
		System.out.println("* * * * *");
		System.out.println("* * * * *");
		System.out.println("* * * * *");
		System.out.println("* * * * *");
	}
	
	//star2 함수
	public void star2() {
		for(int i = 1; i <=5; i++) {
			System.out.println("* * * * *");
		}
	}
}

public class StarTest1 {

	
	
	public static void main(String[] args) {
		//별찍기
		Star st = new Star();
		st.star1();
	}

}
